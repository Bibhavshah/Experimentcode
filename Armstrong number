#include <stdio.h>

int main()
{
  int n,r,c,sum=0,temp;
  printf("Enter a three digit number to : ");
  scanf("%d", &n);
  temp=n;
  while(n>0){
      r=n%10;
      c=r*r*r;
      sum=sum+c;
      n=n/10;
  }
  n=temp;
  if(n==sum)
      printf("The number entered is armstrong number:)");
  
  else
   printf("The number entered is not armstong number:)");
  
    return 0;
}
